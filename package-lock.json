local function UFCXR_fake_script() -- Username.UsernameSet 
	local script = Instance.new('LocalScript', Username)

	script.Parent.Text = game.Players.LocalPlayer.Name
end
coroutine.wrap(UFCXR_fake_script)()
local function FGLVKPD_fake_script() -- USER.Icon 
	local script = Instance.new('LocalScript', USER)

	local frame = script.Parent
	
	
	
	local player = game.Players.LocalPlayer
	
	
	
	local userId = player.UserId
	
	local thumbType = Enum.ThumbnailType.AvatarBust
	
	local thumbSize = Enum.ThumbnailSize.Size60x60
	
	local content, isReady = game.Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	
	
	
	
	frame.Image = content
end
coroutine.wrap(FGLVKPD_fake_script)()
local function TBRDS_fake_script() -- Health.TargetHUD 
	local script = Instance.new('LocalScript', Health)

	game:GetService("RunService").RenderStepped:Connect(function()
		local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
		script.Parent.Parent.HealthbarThing.Size = UDim2.new(humanoid.Health/humanoid.MaxHealth,0,1,0)
		script.Parent.Text = math.floor(humanoid.Health).."/"..humanoid.MaxHealth
	end)
end
coroutine.wrap(TBRDS_fake_script)()



	
	    
	    
	    
	    




